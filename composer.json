{
    "name": "devcake-deventer/country-seeder",
    "description": "This package can be used within laravel to seed a dutch translated country table to the database",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Slinkman",
            "email": "m.slinkman@devcake.nl"
        },
        {
            "name": "Sven Mol",
            "email": "s.mol@devcake.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Devcake\\CountrySeeder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "illuminate/database": "^7.0.0",
        "illuminate/support": "^7.0.0"
    },
    "require-dev": {
        "laravel/framework": "^7.0",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devcake\\CountrySeeder\\CountrySeederServiceProvider"
            ]
        }
    }
}
